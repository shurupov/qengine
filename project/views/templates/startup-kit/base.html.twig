{% import "/common/components/data.html.twig" as data %}<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!--> <html lang="en"> <!--<![endif]-->
<head>
    <title>{% block title %}{{ page.title }}{% if pageTitle|length > 0 %} | {% endif %}{{ settings.title.value }} - разработка сайтов{% endblock %}</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="/i/app2-black.png">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,500,300italic,400italic,500italic,700,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Noto+Sans:400,400italic,700,700italic' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

    <link href='/common/bootstrap/css/bootstrap.min.css' rel='stylesheet' type='text/css'>
    <link href='/common/font-awesome/css/font-awesome.css' rel='stylesheet' type='text/css'>
    <link href='/templates/startup-kit/plugins/flexslider/flexslider.css' rel='stylesheet' type='text/css'>
    <link href='/templates/startup-kit/css/styles.css' rel='stylesheet' type='text/css'>

    <link  href="/plugins/fancybox-3.0/dist/jquery.fancybox.min.css" rel="stylesheet">

    <link  href="/templates/startup-kit/css/additional.css" rel="stylesheet">

    {% block stylesheets %}{% endblock %}

</head>

<body data-spy="scroll" data-target="#page-nav">

<!-- ******HEADER****** -->
<header id="header" class="header">
    <div class="container">
        <h1 class="logo">
            {% if not editMode %}<a href="/">{% endif %}<img src="/i/app2.png" style="height: 35px;">&nbsp;<span class="text">{{ data.textSetting(settings.title, editMode, 'bottom') }}</span>{% if not editMode %}</a>{% endif %}
        </h1><!--//logo-->
        <nav class="main-nav navbar-right" role="navigation">
            <div class="navbar-header">
                <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button><!--//nav-toggle-->
            </div><!--//navbar-header-->
            <div id="navbar-collapse" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    {% for link in topMenu %}
                        <li class="{% if link.uri == requestUri %}active {% endif %}nav-item"><a href="{{ link.uri }}">{{ link.text }}</a></li>
                    {% endfor %}
                </ul><!--//nav-->
            </div><!--//navabr-collapse-->
        </nav><!--//main-nav-->
    </div><!--//container-->
</header><!--//header-->

{% block body %}{% endblock %}

<!-- ******FOOTER****** -->
<footer class="footer">
    <div class="container">
        <div class="divider"></div>
        <div class="footer-bottom text-center">
            <ul class="social-media list-inline">
                <li><a href="{{ settings.vklink.value }}" target="_blank"><i class="fa fa-vk" aria-hidden="true"></i></a></li>
            </ul>

            <small class="copyright"><a href="/">Точный клик</a></small>
        </div>
    </div><!--//container-->
</footer><!--//footer-->

{% block pageSettings %}{% endblock %}

<script src="/common/jquery/jquery-1.12.4.min.js"></script>
<script src="/common/bootstrap/js/bootstrap.min.js"></script>
<script src="/templates/startup-kit/plugins/bootstrap-hover-dropdown.min.js"></script>
<script src="/templates/startup-kit/plugins/back-to-top.js"></script>
<script src="/templates/startup-kit/plugins/jquery-scrollTo/jquery.scrollTo.min.js"></script>
<script src="/templates/startup-kit/plugins/flexslider/jquery.flexslider-min.js"></script>
<script src="/templates/startup-kit/js/main.js"></script>
<script src="/templates/startup-kit/js/home.js"></script>

<script src="http://a.vimeocdn.com/js/froogaloop2.min.js"></script>

<script src="/templates/startup-kit/js/vimeo.js"></script>

<script src="/plugins/fancybox-3.0/dist/jquery.fancybox.min.js"></script>

{% block javascripts %}{% endblock %}

</body>
</html>